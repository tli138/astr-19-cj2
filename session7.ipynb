   "source": [
    "y=exp(x)\n",
    "\n",
    "plt.plot(x, y)\n",
    "plt.xlabel('Time [milliseconds]')\n",
    "plt.ylabel('Awesomeness')\n",
    "\n",
    "plt.savefig('figs/exp_plot.pdf')"
